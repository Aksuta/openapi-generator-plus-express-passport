{{prefix}}enum{{className model.nativeType}}FromJson{{else if (isDate)~}}
{{prefix}}dateFromJson{{else if (isDateTime)~}}
{{prefix}}dateTimeFromJson{{else if (isTime)~}}
{{prefix}}timeFromJson{{else if (isMap)~}}
{{prefix}}mapFromJson{{else~}}
{{prefix}}unsupportedFromJson{{/if}}